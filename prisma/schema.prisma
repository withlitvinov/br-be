generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PersonProfile {
  id  String @id @db.Uuid @default(uuid())
  name String
  birthday DateTime @db.Date
  birthdayMarker Int @default(0) @map("birthday_marker")

  @@map("person_profiles")
}