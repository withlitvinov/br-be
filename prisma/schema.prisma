generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PersonProfile {
  id  String @id @db.Uuid @default(uuid())
  name String
  birthday String @db.VarChar(10) // DD-MM-YYYY, when value for YYYY is "####" it means that year is null

  @@map("person_profile")
}