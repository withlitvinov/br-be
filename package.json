    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@prisma/client": "5.16.1",
    "argon2": "^0.40.3",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.16.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
